mixin optionPageNew
    ul.home-filter__page-option
        each item in listOption
            li.home-filter__page-option-item
                a(href="#")= item

mixin productNew
    each item in listNewProduct
        - const {img, title, price, rates, feedback} = item
        .col-lg-4.col-md-6.home-product-mobile
            +productItem(img, title, price, rates, feedback)
